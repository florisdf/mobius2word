      <algorithm><![CDATA[  $ampl_cm = 0.5;
$ampl = $ampl_cm / 100;
$rho = 7800;
$area_mm = 2;
$area = $area_mm*10^(-6);
$mass = 200;
$freq = 440;
$lin_density = $rho*$area;
$wave_speed = sqrt($mass*9.81/$lin_density);
$ang_freq = 2*Pi*$freq;
$wave_num = $ang_freq/$wave_speed;
$Dxt = maple("$ampl*sin($wave_num*x - $ang_freq*t + Pi)");  ]]></algorithm>
